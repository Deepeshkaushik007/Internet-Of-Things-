#include <DHT.h>
#include <DHT_U.h>

DHT dht(2, DHT11);

void setup() {
  Serial.begin(9600);
  dht.begin();
}

void loop() {
  delay(2000);
  float h = dht.readHumidity();
  float tc = dht.readTemperature();
  float tf = dht.readTemperature(true);

  Serial.print(h);
  Serial.print(" % Humidity \t");
  Serial.print(tc);
  Serial.print(" °C Temperature \t");
  Serial.print(tf);
  Serial.print(" °F Temperature \t");
  Serial.println("");
  }
